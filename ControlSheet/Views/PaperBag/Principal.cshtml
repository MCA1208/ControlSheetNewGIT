@{
    Layout = "~/Views/Shared/_LayoutMaster.cshtml";
}
<script type="text/javascript">

    $(document).ready(function () {

        $('#liContact').css('background-color', '');
        $('#liHome').css('background-color', '');
        $('#liSheet').css('background-color', '');
        $('#liPaperBag').css('background-color', '#000000');

    });

</script>

<div class="card">
    <div class="card-body" style="color:black; background-color:aqua; font-family:fantasy; font-size:xx-large">
        Paper Bag
    </div>
</div>


<div class="card-body">
    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#ModifyProfileModal"><i class="fas fa-arrow-circle-right"></i> Modifical Perfil </button>
</div>

<div>
    <div class="table-responsive">
        <table id="tblPaperBag" class="table" style="color: black; text-align:left; ">
            <thead class="thead-light">
                <tr>
                    <th scope="col">Perfil</th>
                    <th scope="col">Una pasión</th>
                    <th scope="col">Algo que me gusta</th>
                    <th scope="col">Descripcón</th>
                    <th scope="col">Ver</th>
                </tr>
            </thead>
        </table>
    </div>

</div>


<!-- Modal -->

<div class="modal fade" id="ModifyProfileModal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel">Modificar Perfil</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <table class="table table-striped" style="color:black">
                    <tr>
                        <th>Foto - Perfil</th>
                        <td>
                            <input style="width:300px" id="txtCompany" type="file" />
                        </td>

                    </tr>
                    <tr>
                        <th>Foto - Una pasión</th>
                        <td>
                            <input style="width:300px" id="txtNewEmail" type="file" />
                        </td>
                    </tr>
                    <tr>
                        <th>Foto - Algo que me gusta</th>
                        <td>
                            <input style="width:300px" id="txtNewPass" type="file" />
                        </td>

                    </tr>
                    <tr>
                        <th>Datos Personales</th>
                        <td>
                            <input style="width:300px" id="txtNewPassRepeat" type="password" />
                        </td>

                    </tr>

                </table>
                @*</div>*@
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal"><i class="fas fa-window-close"></i> Cerrar</button>
                <button type="button" class="btn btn-primary" onclick="createUserAdmin();"><i class="fas fa-user-alt"></i> Crear</button>
            </div>
        </div>
    </div>
</div>

<link href="~/Scripts/AlertMessage/alertifyjs/css/alertify.css" rel="stylesheet" />
<link href="~/Scripts/AlertMessage/alertifyjs/css/themes/default.css" rel="stylesheet" />
<link href="~/Scripts/bootstrap-4.2.1/css/bootstrap.min.css" rel="stylesheet" />

<script src="~/Scripts/jquery-3.4.0/jquery-3.4.0.min.js"></script>
<script src="~/Scripts/bootstrap-4.2.1/js/bootstrap.min.js"></script>
<script src="~/Scripts/fontawesome/all.js"></script>
<script src="~/Scripts/jquery-3.4.0/BlockUI/jquery.blockUI.min.js"></script>
<script src="~/Scripts/Controller/LoginController.js"></script>
<script src="~/Scripts/AlertMessage/alertifyjs/alertify.js"></script>
<script src="~/Scripts/GlobalFunc/General.js"></script>
<script src="~/Scripts/GlobalFunc/URLController.js"></script>
